CSRCS = $(filter-out test1_main.c,$(wildcard *.c))
MAINSRC = test1_main.c

CONFIG_KCAPPS_TEST1_PROGNAME ?= test1$(EXEEXT)
PROGNAME = $(CONFIG_KCAPPS_TEST1_PROGNAME)

include $(APPDIR)/Application.mk

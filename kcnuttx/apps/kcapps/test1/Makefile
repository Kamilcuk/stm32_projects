
-include $(TOPDIR)/Make.defs

CONFIG_KCAPPS_TEST1_PRIORITY ?= SCHED_PRIORITY_DEFAULT
CONFIG_KCAPPS_TEST1_STACKSIZE ?= 2048

APPNAME = test1

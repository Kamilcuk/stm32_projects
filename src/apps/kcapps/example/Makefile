
-include $(TOPDIR)/Make.defs

CONFIG_KCAPPS_EXAMPLE_PRIORITY ?= SCHED_PRIORITY_DEFAULT
CONFIG_KCAPPS_EXAMPLE_STACKSIZE ?= 2048

APPNAME = example
PRIORITY = $(CONFIG_KCAPPS_EXAMPLE_PRIORITY)
STACKSIZE = $(CONFIG_KCAPPS_EXAMPLE_STACKSIZE)

ASRCS = $(wildcard *.s)
CSRCS = $(filter-out example_main.c,$(wildcard *.c))
MAINSRC = example_main.c

CONFIG_KCAPPS_EXAMPLE_PROGNAME ?= example$(EXEEXT)
PROGNAME = $(CONFIG_KCAPPS_EXAMPLE_PROGNAME)

include $(APPDIR)/Application.mk
